# Called every frame. 'delta' is the elapsed time since the previous frame.

func add_tile(x, y, name):
	var id = scenes_with_id[name]
	var tile_x = 0
	var tile_y = 0
	
	if x != 0:
		tile_x = x / 16
	if y!= 0:
		tile_y = y / 16
		
	tile_map.set_cell(layer, Vector2i(tile_x, tile_y), 0, Vector2i(0, 0), id)
	
	if last_object != name:
		last_object = name
		layer += 1
	
